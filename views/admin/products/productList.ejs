                                                                                                                        <input class="btn" style="background-color:blue;width: 15px; height: 15px;red; border: thin solid black" disabled>
                                                                                                                        <label><%= __('Deposited') %></label>
                                                                                                                    </div>
                                                                                                                    <div class="col-md-2">
                                                                                                                        <input class="btn" style="background-color:rgb(50,197,210);width: 15px; height: 15px;red; border: thin solid black" disabled>
                                                                                                                        <label><%= __('Consigned') %></label>
                                                                                                                    </div>
                                                                                                                </div>
                                                                                                            </form>
                                                                                                            </div>
                                                                                                        </div>
