                                                                                                            <div class="table-toolbar">
                                                                                                                <form class="horizontal-form">
                                                                                                                <div class="row">
                                                                                                                    <div class="col-md-2">
                                                                                                                        <input class="btn" style="background-color:white;width: 15px; height: 15px;red; border: thin solid black" disabled>
                                                                                                                        <label><%= __('Available') %></label>
                                                                                                                    </div>
                                                                                                                    <div class="col-md-2">
                                                                                                                        <input class="btn" style="background-color:red;width: 15px; height: 15px;red; border: thin solid black" disabled>
                                                                                                                        <label><%= __('Sold') %></label>
                                                                                                                    </div>
                                                                                                                    <div class="col-md-2">
