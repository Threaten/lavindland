                                                                                                                <%= products[i].customer.name %> -
                                                                                                                <%= __('Deposited: ') %>
                                                                                                                <%= products[i].deposit.toLocaleString() %> VND</td>
                                                                                                                <% } else {%>
                                                                                                                    <td></td>
                                                                                                                    <% } %>
                                                                                                                    <th>
                                                                                                                        <%= products[i].note %>
                                                                                                                    </th>
                                                                                                                </tr>
                                                                                                                <% } %>
                                                                                                            </tbody>
                                                                                                        </table>
                                                                                                    </div>
                                                                                                    <div class="tab-pane" id="tab_1">
